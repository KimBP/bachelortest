public class OptimizedTimes {
  private int redTime;
  private int greenTime;
  private int blueTime;

  public OptimizedTimes(int redTime, int greenTime, int blueTime) {
    this.redTime = redTime;
    this.greenTime = greenTime;
    this.blueTime = blueTime;
  }

  public int getRed() {
    return redTime;
  }

  public int getGreen() {
    return greenTime;
  }

  public int getBlue() {
    return blueTime;
  }
}
